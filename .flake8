[flake8]
exclude = 
    .gitignore
    .pre-commit-config.yaml
    poetry.lock
    pyproject.toml
    README.md
    __pycache__
    .poetry
    .pytest_cache
    .DS_Store
    tests/*
    dist/*
extend-ignore =
    E501
max-line-length = 79
per-file-ignores = 
    */__init__.py:F401
    gspread_helpers/range_name/validations.py:F821
